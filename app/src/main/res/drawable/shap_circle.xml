<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- 填充颜色 -->
    <solid android:color="@color/bg_color_first" />

    <!-- 矩形的圆角半径 -->
    <corners android:radius="5dp" />

    <!--线的宽度，颜色-->
    <stroke
        android:width="3dp"
        android:color="#DCDCDC"
        />

    <gradient
        android:endColor="@color/bg_color_second"
        android:startColor="@color/bg_color_first" />

</shape>
