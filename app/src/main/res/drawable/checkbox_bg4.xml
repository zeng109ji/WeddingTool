<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_checked="false" android:state_pressed="false" android:drawable="@drawable/item_add2" />
    <item android:state_enabled="true" android:state_checked="false" android:state_pressed="true" android:drawable="@drawable/item_add1" />
    <item android:state_enabled="true" android:state_checked="true" android:state_pressed="false" android:drawable="@drawable/item_add2" />
    <item android:state_enabled="true" android:state_checked="true" android:state_pressed="true" android:drawable="@drawable/item_add1" />
    <item android:drawable="@drawable/item_add2" />
</selector>
<!-- 为了能在ExpandableListView的Group分组栏目里添加按钮而改写的checkbox图形，因为ImageButton和Button加进Grouplist的布局里，都会导致Grouplist栏目不响应点击状态，只有checkbox可以不影响 -->